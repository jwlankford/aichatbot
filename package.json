{
  "name": "aichatbot",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs --fix --ignore-path .gitignore",
    "deploy": "npm run build && gh-pages -d dist --add --repo https://github.com/jwlankford/aichatbot.git",
    "test": "vitest --environment=jsdom"
  },
  "dependencies": {
    "core-js": "^3.8.3",
    "firebase": "^11.8.1",
    "marked": "^12.0.1",
    "vue": "^3.4.21"
  },
  "devDependencies": {
    "@babel/eslint-parser": "^7.27.5",
    "@types/node": "^22.15.29",
    "@vitejs/plugin-vue": "^5.2.4",
    "@vue/eslint-config-prettier": "^9.0.0",
    "@vue/test-utils": "^2.4.6",
    "@vue/tsconfig": "^0.7.0",
    "eslint": "^9.28.0",
    "eslint-plugin-vue": "^9.23.0",
    "gh-pages": "^6.3.0",
    "prettier": "^3.2.5",
    "shx": "^0.4.0",
    "vite": "^5.2.8",
    "vitest": "^3.2.1",
    "jsdom": "^24.0.0"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/vue3-essential",
      "eslint:recommended"
    ],
    "parserOptions": {
      "parser": "@babel/eslint-parser"
    },
    "rules": {}
  },
  "type": "module",
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead",
    "not ie 11"
  ]
}
